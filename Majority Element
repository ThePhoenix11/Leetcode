class Solution {
public:
    int majorityElement(vector<int>& nums) {
        if(nums.size()==1){return nums[0];}
        int n=nums[0], count=1;
        for(int i=1;i<nums.size();i++)
        {
            if(nums[i]==n){count++;}
            else if(nums[i]!=n)
            {
                count--;
                if(count<=0){count =1;n=nums[i];}
            }
        }
        return n;
    }
};
