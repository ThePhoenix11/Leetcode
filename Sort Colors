class Solution {
public:
    void sortColors(vector<int>& n) {
        int low,high,mid;
        low=0;mid=0;high=n.size()-1;
        while(mid<=high)
        {
            
            if(n[mid]==0){swap(n[low],n[mid]);mid++;low++;}
            else if(n[mid]==1){mid++;}
            else if(n[mid]==2){swap(n[high],n[mid]);high--;}
            
        }
    }
};
